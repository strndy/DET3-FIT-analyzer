<Window x:Class="Det3FitAutotuneGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:det3FitAutotuneGui="clr-namespace:Det3FitAutotuneGui"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="Import" Width="100">
                <Label Content="Content goes here..." />
            </TabItem>
            <TabItem Header="Analyze" Width="100">
                <TabControl>
                    <TabItem Header="Target AFR" Width="80">
                        <det3FitAutotuneGui:ValuesGrid Name="TargetAfr"></det3FitAutotuneGui:ValuesGrid>
                    </TabItem>
                    <TabItem Header="Actual AFR" Width="80"></TabItem>
                    <TabItem Header="AFR correction" Width="80"></TabItem>
                    <TabItem Header="VETable" Width="80"></TabItem>
                </TabControl>
            </TabItem>
            <TabItem Header="Export" Width="100"/>
        </TabControl>
    </Grid>
</Window>
